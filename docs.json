          {
            "group": "Integration",
            "pages": [
              "developer/quickstart",
              "developer/authentication",
              "developer/making-requests"
            ]
          },
          {
            "group": "Code Examples",
            "pages": [
              "developer/examples/nodejs",
              "developer/examples/python",
              "developer/examples/go",
              "developer/examples/rust"
